# Example environment variables for development
# Copy this file to `.env.local` and replace placeholder values with real credentials.

# Next.js / App
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=
NEXTAUTH_SECRET=
NEXTAUTH_URL=

# Database
DATABASE_URL=

# Cloudinary
CLOUDINARY_URL=
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=

# OpenAI
OPENAI_API_KEY=

# Meta / WhatsApp Cloud API (required for WhatsApp features)
META_WHATSAPP_PHONE_NUMBER_ID=
META_WHATSAPP_ACCESS_TOKEN=
META_GRAPH_API_VERSION=v22.0
# Optional - if your account uses a business id
META_WHATSAPP_BUSINESS_ACCOUNT_ID=
META_WHATSAPP_BUSINESS_ID=

# Optional for token refresh / webhook verification
META_APP_ID=
META_APP_SECRET=
META_WEBHOOK_VERIFY_TOKEN=

# WhatsApp script helpers (used by scripts/whatsapp/*.js)
TEMPLATE_NAME=deep_test_kashmir_27500
TEMPLATE_LANGUAGE=en
# Either MEDIA_ID (uploaded media id) or HEADER_IMAGE (public URL) depending on script
MEDIA_ID=
HEADER_IMAGE=
# Single recipient (for TO) or comma separated list for RECIPIENTS
TO=+919978783238
RECIPIENTS=+919978783238

# Other keys used in the project
STRIPE_API_KEY=
WHATSAPP_FLOW_PRIVATE_KEY=
WHATSAPP_FLOW_KEY_PASSPHRASE=
WHATSAPP_CATALOG_ID=

# Debug
WHATSAPP_DEBUG=0
